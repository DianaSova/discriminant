from math import *

# объявление функции
def solve(a, b, c):

    d = b ** 2 - 4 * a * c 
    
    if d > 0:
        
        if a < 0 or c < 0:
            return (- b + sqrt(d)) / (2 * a), (- b - sqrt(d)) / (2 * a)
        else:    
            return (- b - sqrt(d)) / (2 * a), (- b + sqrt(d)) / (2 * a)
        
    if d == 0:
        return - b / (2 * a)

# вызываем функцию
a, b, c = int(input()), int(input()), int(input())
x1, x2 = solve(a, b, c)

print(x1, x2)
